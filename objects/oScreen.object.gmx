            <string>///Screen Shakiation
//if shake_timer is positive, shakes the screen and decrements shake_timer

if (shake_timer &gt; 0)
{
    shake = (random_range(-shake_timer, shake_timer))/2;    //div by 2 so shake not to drastic
    shake_timer -= 1;
}

timer += timer_switch;
//sin = expand, cos = contract
xscale = abs(sin(timer/20));
if (xscale &gt;= 0.99)
{
    xscale = 1;
    timer_switch = 0;
}
if (keyboard_check_pressed(vk_enter))
{
    timer_switch = -1;
}
